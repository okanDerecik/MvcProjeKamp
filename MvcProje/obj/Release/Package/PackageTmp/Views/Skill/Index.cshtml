@using EntityLayer.Concreate
@model List<Skill>

@{
    Layout = null;
}


<!DOCTYPE html>
<html>
<head>
    <title>Yetenek Kartım</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Skills Profile Widget template Responsive, Login form web template,Flat Pricing tables,Flat Drop downs  Sign up Web Templates, Flat Web Templates, Login sign up Responsive web template, SmartPhone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!--Custom Theme files-->
    <link href="~/AdminLTE-3.0.4/web/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <!--//Custom Theme files -->
    <!--web font-->
    <link href='//fonts.googleapis.com/css?family=Questrial' rel='stylesheet' type='text/css'>
    <link href='//fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,700italic,800,800italic' rel='stylesheet' type='text/css'>
    <!--//web font-->
    <!--circle-chart-->

    <link rel="stylesheet" href="~/AdminLTE-3.0.4/plugins/fontawesome-free/css/all.min.css">

    <script src="~/AdminLTE-3.0.4/web/js/jquery-1.11.1.min.js"></script>
    <!--//circle-chart-->
    <!--ResponsiveTabs-->
    <script src="~/AdminLTE-3.0.4/web/js/easyResponsiveTabs.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('#horizontalTab').easyResponsiveTabs({
                type: 'default', //Types: default, vertical, accordion
                width: 'auto', //auto or any width like 600px
                fit: true   // 100% fit in a container
            });
        });
    </script>
    <!--//ResponsiveTabs-->
</head>
<body>
    <!-- main -->
    <div class="main">
        <h1>Yetenek Kartı Sayfam</h1>
        <div class="main-info w3l">
            <div class="main-row">
                <!-- main-row-one -->
                <div class="profile-grid logo wthree">
                    <img src="~/AdminLTE-3.0.4/web/WhatsApp Image 2022-08-30 at 00.03.24.jpeg" width="30%" alt="">
                    <h2>Okan Derecik </h2>
                    <p>Yazılım Geliştirici</p>

                </div>
                <h3 class="title">Yetenekler</h3>
                <div class="col-md-6 skills-right bar_group">
                    @foreach (var item in Model)
                    {
                        <div class='bar_group__bar thin' label='@item.SkillName &nbsp;&nbsp; @item.SkillRange%' value='@item.SkillRange'></div>
                    }
                </div>
                <!--bar-js-->
                <script src="~/AdminLTE-3.0.4/web/js/bars.js"></script>
                <div class="social-icons agileits">
                    <ul>
                        <li><a href="https://www.linkedin.com/in/okan-derecik-a656a2235" class="be"></a></li>
                    </ul>

                </div><br />
                <a href="/Heading/Index/"style="margin-left:255px;color:white">Geri Dön</a>
                <div class="clear"> </div>
            </div>
        </div>
        <!-- copyright -->
    </div>
    <!-- main -->

    <script src="~/AdminLTE-3.0.4/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>

    <script src="~/AdminLTE-3.0.4/dist/js/adminlte.js"></script>
    <script src="~/AdminLTE-3.0.4/plugins/chart.js/Chart.min.js"></script>
    <script src="~/AdminLTE-3.0.4/dist/js/demo.js"></script>
    <script src="~/AdminLTE-3.0.4/dist/js/pages/dashboard3.js"></script>
</body>
</html>

